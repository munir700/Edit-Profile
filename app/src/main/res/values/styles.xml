<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TextInputLayout">
        <item name="android:layout_marginBottom">@dimen/textinputlayout_input_bottom_margin</item>
        <item name="android:layout_marginLeft">@dimen/textinput_layout_input_margin</item>
        <item name="android:layout_marginRight">@dimen/textinput_layout_input_margin</item>
    </style>

    <style name="lable_text_appearance" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textColor">@color/colorPrimary</item>

    </style>
</resources>
